<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<header>
  <%= image_tag("logo.jpg", :alt => "DireWolf Jobs") %>
</header>

<% flash.each do |flash_type, message| %>
    <%= message %>
<% end %>

<%= yield %>

</body>
</html>
